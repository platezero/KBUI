ify-start column fill-height mb-3>\n          <v-flex xs12>\n            <div class=\"headline\">Cause</div>\n            <vue-editor ref=\"cause\" v-model=\"article.quill.cause\" :editorOptions=\"editorSettings\"></vue-editor>\n          </v-flex>\n        </v-layout>\n        <v-layout align-start justify-start column fill-height mb-3>\n          <v-flex xs12>\n            <div class=\"headline\">Root Cause</div>\n            <vue-editor\n              ref=\"causeRoot\"\n              v-model=\"article.quill.causeRoot\"\n              :editorOptions=\"editorSettings\"\n            ></vue-editor>\n          </v-flex>\n        </v-layout>\n        <v-layout align-start justify-start column fill-height mb-3>\n          <v-flex xs12>\n            <div class=\"headline\">Solution</div>\n            <vue-editor\n              ref=\"solution\"\n              v-model=\"article.quill.solution\"\n              :editorOptions=\"editorSettings\"\n            ></vue-editor>\n          </v-flex>\n        </v-layout>\n        <v-layout align-start justify-start column fill-height mb-3>\n          <v-flex xs12>\n            <div class=\"headline\">Related Resources</div>\n            <vue-editor\n              ref=\"resourcesRelated\"\n              v-model=\"article.quill.resourcesRelated\"\n              :editorOptions=\"editorSettings\"\n            ></vue-editor>\n          </v-flex>\n        </v-layout>\n        <v-layout row>\n          <v-flex xs12>\n            <modal-confirm\n              title=\"ยืนยันการบันทึกข้อมูล\"\n              description=\"กรุณาตรวจสอบข้อมูลก่อนการบันทึก\"\n              v-on:onConfirm=\"onConfirm\"\n            ></modal-confirm>\n            <v-btn color=\"success\" @click=\"saveArticle\">บันทึก</v-btn>\n          </v-flex>\n        </v-layout>\n      </v-layout>\n      <v-layout v-show=\"readOnly\" align-center justify-start column>\n        <v-layout align-center justify-start column fill-height mb-3>\n          <v-flex xs12>\n            <v